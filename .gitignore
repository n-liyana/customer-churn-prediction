nano .gitignore  # For Linux/Mac
notepad .gitignore  # For Windows

# Ignore Python cache files
__pycache__/
*.py[cod]
*.pyo

# Ignore virtual environment
env/
venv/

# Ignore Jupyter Notebook checkpoints
.ipynb_checkpoints/

# Ignore log files
*.log

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore outputs or temp files
outputs/
*.png

# Ignore model files (optional)
*.pkl
*.h5
